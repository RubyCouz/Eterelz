<form method="post" action="{{ path('eter_user_delete', {'id': eter_user.id}) }}" onsubmit="return confirm('Etes-vous certain(e) de vouloir supprimer votre profil ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ eter_user.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>